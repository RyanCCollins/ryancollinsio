    "expect-jsx": "^2.6.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^v2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.4",
    "functional-components": "^0.0.5",
    "graphql": "^0.8.1",
